<odoo>
    <data>

        <record id="crm_tin_inherit_new" model="ir.ui.view">
            <field name="name">Crm Lead</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_lead_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tag_ids']" position="after">
                    <field name="seq_no"/>
                    <field name="sale_types_id" required="1"/>
                </xpath>
            </field>
        </record>
        # sequence Generation
        <record id="sequence_generation" model="ir.sequence">
            <field name="name">Crm Lead</field>
            <field name="code">crm.lead</field>
            <field name="prefix">/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        #Kanban Inherited
        <record id="crm_lead_kanban_inherit_sg" model="ir.ui.view">
            <field name="name">crm.lead.kanban.inherited</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_case_kanban_view_leads"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@class='oe_kanban_content flex-grow-1']/div[2]" position="after">
                    <field name="seq_no"/><br/>
                </xpath>
            </field>
        </record>


        <!-- respartner add new field   -->
        <record id="customer_tin_inherit" model="ir.ui.view">
            <field name="name">Res Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="customer_id" attrs="{'invisible': [('vendor_id','not in',False)]}"/>
                    <field name="vendor_id" attrs="{'invisible': [('customer_id','not in',False)]}"/>
                    <field name="cus_tin"/>
                    <field name="pan_card_info"/>
                    <field name="alternative_email"/>
                    <!--                <field name="seq_no"/>-->
                </xpath>
            </field>
        </record>

        <record id="surya_graphics_customer_sequence" model="ir.sequence">
            <field name="name">Customer ID generation</field>
            <field name="code">res.partner.customer</field>
            <field name="prefix">SG</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <record id="surya_graphics_supplier_sequence" model="ir.sequence">
            <field name="name">Supplier ID generation</field>
            <field name="code">res.partner.vendor</field>
            <field name="prefix">SG</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <record id="offset_printing_quotation" model="ir.sequence">
            <field name="name">Offset Printing Quotation</field>
            <field name="code">offset.printing.quotation</field>
            <field name="prefix">OS/Q/22-23/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <record id="view_on_boarding_form" model="ir.ui.view">
            <field name="name">Sale Quotation</field>
            <field name="model">sale.quotation</field>
            <field name="arch" type="xml">
                <form string="On Boarding">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="1"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_on_boarding_tree" model="ir.ui.view">
            <field name="name">Sale Quotation tree</field>
            <field name="model">sale.quotation</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="off_boarding_action" model="ir.actions.act_window">
            <field name="name">Sale Quotation</field>
            <field name="res_model">sale.quotation</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="view_in_asle_quotation" model="ir.ui.view">
            <field name="name">Sale Type</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='l10n_in_gst_treatment']" position="after">
                    <field name="sale_type_id" options="{'no_open': True, 'no_create': True}" readonly="1"/>
                </xpath>
            </field>
        </record>

        <record id="sale.action_quotations_with_onboarding" model="ir.actions.act_window">
            <field name="name">Offset Quotations</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sale.order</field>
            <field name="view_id" ref="sale.view_quotation_tree_with_onboarding"/>
            <field name="view_mode">tree,kanban,form,calendar,pivot,graph,activity</field>
            <field name="search_view_id" ref="sale.sale_order_view_search_inherit_quotation"/>
            <field name="context">{'default_sale_type_id': 1}</field>
            <field name="domain">[('sale_type_id', '=', 1),('state', '=', 'sale')]</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new Offset Printing quotation, the first step of a new sale!
                </p>
                <p>
                    Once the Offset Printing quotation is confirmed by the customer, it becomes a sales order.
                    <br/>
                    You will be able to create an invoice and collect the payment.
                </p>
            </field>
        </record>

